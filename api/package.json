    "@nestjs/swagger": "^11.1.4",
    "@nestjs/typeorm": "^11.0.0",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jest-mock-extended": "^3.0.7",
    "pg": "8.14.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
