  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "lint": "ts-standard --report stylish --fix src",
    "prettier": "prettier --write src",
    "format": "npm run lint && npm run prettier",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main/",
    "test:e2e": "jest --runInBand --coverageDirectory .tmp_coverage/e2e test/e2e",
    "test:unit": "jest --coverageDirectory .tmp_coverage/unit --collectCoverageFrom=src/core/*.ts test/unit",
    "pre-commit": "npm run format",
    "compile": "tsc -b --verbose"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.16.0",
    "@nestjs/common": "^11.0.20",
