    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.0",
    "jest": "29.7.0",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "serverless-offline": "^14.4.0",
    "supertest": "^7.1.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
